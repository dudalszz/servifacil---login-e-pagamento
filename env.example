# Configurações da API
REACT_APP_API_URL=http://localhost:8080/api
REACT_APP_API_TIMEOUT=10000

# Configurações de autenticação
REACT_APP_JWT_SECRET=your-jwt-secret-here
REACT_APP_TOKEN_EXPIRY=24h

# Configurações de upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# Configurações de ambiente
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# URLs externas
REACT_APP_TERMS_URL=https://servifacil.com/terms
REACT_APP_PRIVACY_URL=https://servifacil.com/privacy
REACT_APP_SUPPORT_EMAIL=support@servifacil.com

# Configurações de notificação
REACT_APP_VAPID_PUBLIC_KEY=your-vapid-public-key
REACT_APP_FCM_SERVER_KEY=your-fcm-server-key

